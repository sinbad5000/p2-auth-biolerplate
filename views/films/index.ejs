<body id="films">
    <div class="container">
        <h2>All the Studio Gihlbi films in one place!</h2>
        <ul>
            <% films.forEach(function(film) { %>
                <li class="list"><a href="/films/<%= film.id %>"><%= film.title %></a></li>
                <form method="POST" action="/profile">
                    <input hidden type="text" name="title" value="<%= film.title %>">
                    <button class="btn btn-primary" type="submit">Add to Profile</button>
                  </form>
            <% }) %>
        </ul>
    </div> 
</body>